<channel>
  <title>#{blogTitle}
  <link>#{renderer RootR}
  <description>#{description}
  <atom:link href=#{renderer RssR} rel=self type=application/rss+xml>
  $forall postEnt <- posts
    <item>
      <title>#{postTitle $ entityVal postEnt}
      <guid>#{renderer $ router $ entityVal postEnt}
      <link>#{renderer $ router $ entityVal postEnt}
      <description>#{L.toStrict $ renderHtml $ postBody $ entityVal postEnt}