div.sidebar-tag-list {
    background-color: white;
    overflow: hidden;
    padding: 20px;
    border: 1px solid black;
    .tag {
	background-color: #EEE;
	padding: 1px;
	margin: 0px;
	line-height: 2.0;
	border-bottom: 1px solid;
	border-right: 1px solid;
	white-space: nowrap;

	a {
	    color: black;
	}

	a:hover {
	    color: #444;
	}
    }
}